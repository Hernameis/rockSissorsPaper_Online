package com.mukzzibba.server;

import java.io.IOException;

import com.mukzzibba.client.data.UserData;

public class StreamOpener {
	
	public static void openInStream(Socket sock){
		try {
			Server.is=ServerData.sock.getInputStream();
		} catch (IOException e) {
			e.printStackTrace();
		}
	}
	
	public static void openOutStream(Socket sock){
		try {
			UserData.mainSocket.getOutputStream();
		} catch (IOException e) {
			e.printStackTrace();
		}
	}

}
