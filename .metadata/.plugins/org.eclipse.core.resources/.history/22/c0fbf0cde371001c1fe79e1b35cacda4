package com.mukzzibba.client.action;

import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;

import com.mukzzibba.client.controller.PanelController;
import com.mukzzibba.client.data.UserData;


public class Login{

	public static void login(){
		System.out.println("로그인 누름");
		verification();
		if(UserData.userCheck) {
			PanelController.mainToRobbyPanel();
		} else {
			System.out.println("로그인 정보가 잘못되엇ㅅ브ㄴ");
		}
	}
	
	public static void verification(){
		OpenAndCloseSocket.OpenSocket();
		
		
		
		OpenAndCloseSocket.CloseSocket();
	}
}
