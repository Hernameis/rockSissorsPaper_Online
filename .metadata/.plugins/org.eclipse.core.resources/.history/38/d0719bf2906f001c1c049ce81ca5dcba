package com.tanktank.server;

import java.io.IOException;
import java.net.*;

public class Main {

	public static void main(String[] args) {
		ServerSocket serv;
		Socket sock;
		try {
			serv=new ServerSocket(5000);
			sock=serv.accept();
		} catch (IOException e) {
			e.printStackTrace();
		}
		
		
	}

}
