package com.mukzzibba.client.panel;

import java.awt.Button;
import java.awt.Panel;
import java.awt.TextField;

import com.mukzzibba.client.button.ExitButton;

public class StartPanel extends Panel{
	TextField serverIp;
	TextField nickname;
	TextField password;
	
	Button loginButton;
	Button registerButton;
	Button exitButton;
	
	public StartPanel() {
		serverIp=new TextField("127.0.0.1");
		nickname=new TextField("닉네임");
		exitButton=new ExitButton("나가기");
		
		add(serverIp);
		add(exitButton);
	}
}
