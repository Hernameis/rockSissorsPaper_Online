package com.mukzzibba.client.controller;

import java.awt.GridLayout;
import java.awt.Panel;

import com.mukzzibba.client.button.ButtonWithSendingMsg;
import com.mukzzibba.client.button.DrawButton;

public class DrawPanel extends Panel{
	DrawButton rock;
	DrawButton scissors;
	DrawButton paper;
	
	public DrawPanel() {
		this.setLayout(new GridLayout(1,3));
		rock=new DrawButton("rock", "rock");
		scissors=new DrawButto ("scissors", "scis");
		paper=new DrawButton("paper",  "pape");
		
		add(rock);
		add(scissors);
		add(paper);
	}
}
