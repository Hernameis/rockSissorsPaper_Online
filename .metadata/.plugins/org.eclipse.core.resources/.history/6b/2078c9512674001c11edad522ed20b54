package com.mukzzibba.server;

import java.util.Comparator;
import java.util.Map.Entry;
import java.util.Set;

import com.mukzzibba.server.userDb.UserInfo;

public class RankingComparator implements Comparator<UserInfo>{

	@Override
	public int compare(Set<Entry<String, UserInfo>> o1, Set<Entry<String, UserInfo>> o2) {
		if (o1. < o2.getValue().score){
			return 1;
		} else if (o1.getValue().score == o2.getValue().score) {
			return 0;
		} else {
			return -1;
		}
	}
