package com.mukzzibba.client.socketNetwork;

import java.io.IOException;
import java.net.Socket;
import java.net.UnknownHostException;

import com.mukzzibba.client.UserData;

public class MakeSocket extends Thread{
	
	@Override
	public void run() {
		UserData.mainSocket=
		try {
			UserData.mainSocket=new Socket(UserData.serverIp, UserData.port);
		} catch (UnknownHostException e) {
			e.printStackTrace();
		} catch (IOException e) {
			e.printStackTrace();
		}
		while(UserData.end==false){
			try {
				Thread.sleep(1);
			} catch (InterruptedException e) {
				e.printStackTrace();
			}
		}
	}
}
