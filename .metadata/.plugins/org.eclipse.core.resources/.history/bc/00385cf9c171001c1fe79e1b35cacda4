package com.mukzzibba.client.button;

import java.awt.Button;
import java.awt.Frame;
import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;

import com.mukzzibba.client.action.ExitAction;



public class ExitButton extends Button implements ActionListener{
	
	public ExitButton(String name, Frame frame) {
		super(name);
		exit=new ExitAction();
		addActionListener(exit);
	}

	@Override
	public void actionPerformed(ActionEvent e) {
		
	}
}
