package com.mukzzibba.client.controller;

import java.awt.Panel;


public class MainPanel extends Panel{
	public static MainPanel singleTone;
	
	private MainPanel() {
		
	}
	
	public static MainPanel getInstance() {
		if(singleTone==null) {
			singleTone=new MainPanel();
		}
		return singleTone;
	}
	
	
}
